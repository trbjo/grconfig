// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "t",
        "command": "list.focusDown",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "j",
        "command": "-list.focusDown",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "c",
        "command": "list.focusUp",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "k",
        "command": "-list.focusUp",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "j",
        "command": "notebook.cell.copy",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "c",
        "command": "-notebook.cell.copy",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "q",
        "command": "notebook.cell.cut",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "x",
        "command": "-notebook.cell.cut",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "backspace",
        "command": "notebook.cell.delete",
        "when": "notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "d d",
        "command": "-notebook.cell.delete",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "ctrl+t",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+; s",
        "command": "-jupyter.insertCellBelowPosition",
        "when": "editorTextFocus && isWorkspaceTrusted && jupyter.hascodecells && !notebookEditorFocused"
    },
    {
        "key": ".",
        "command": "notebook.cell.insertCodeCellBelow",
        "when": "notebookCellListFocused && !inputFocus"
    },
    {
        "key": "ctrl+enter",
        "command": "-notebook.cell.insertCodeCellBelow",
        "when": "notebookCellListFocused && !inputFocus"
    },
    {
        "key": "ctrl+.",
        "command": "notebook.cell.insertCodeCellAbove",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
    {
        "key": "a",
        "command": "-notebook.cell.insertCodeCellAbove",
        "when": "config.jupyter.enableKeyboardShortcuts && notebookEditorFocused && !inputFocus && notebookType == 'jupyter-notebook'"
    },
]
