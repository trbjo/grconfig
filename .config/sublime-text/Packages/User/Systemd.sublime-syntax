%YAML 1.2
---
name: Systemd
file_extensions: [service, network]
scope: source.systemd

contexts:
  main:
    - match: '^([A-Za-z]+(?=([\=][\-]?)))'
      scope: keyword.other
    - match: '^\[[A-Z][a-z]+\]$'
      scope: entity.name.filename.find-in-files
    - match: '(\\\w)?'
      scope: entity.name
    - match: '\\'
      scope: constant.character.escape
    - match: '\#.*'
      scope: comment.number-sign
